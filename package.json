{
  "type": "module",
  "version": "4.3.0",
  "private": true,
  "packageManager": "pnpm@10.8.0+sha512.0e82714d1b5b43c74610193cb20734897c1d00de89d0e18420aebc5977fa13d780a9cb05734624e81ebd81cc876cd464794850641c48b9544326b5622ca29971",
  "license": "MIT",
  "scripts": {
    "build": "pnpm -r --filter 'packages/**' run build && npm run client:build",
    "dev:prepare": "pnpm -r --filter 'packages/**' run dev:prepare && nuxi prepare client && nuxi prepare playground",
    "client:build": "nuxi generate client",
    "client:dev": "nuxi dev client --port 3030",
    "dev": "npm run play:dev",
    "play:dev": "nuxi dev playground",
    "play:prod": "npm run build && nuxi dev playground",
    "release": "pnpm build && bumpp && pnpm -r publish",
    "lint": "eslint . --fix",
    "test": "vitest run",
    "test:types": "npx nuxi typecheck",
    "test:watch": "vitest watch"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.11.0",
    "@nuxt/content": "3.4.0",
    "@nuxt/devtools": "^2.3.2",
    "@nuxt/module-builder": "^1.0.0",
    "@nuxt/test-utils": "^3.17.2",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/sitemap": "^7.2.10",
    "@types/diff": "^7.0.2",
    "bumpp": "^10.1.0",
    "eslint": "^9.24.0",
    "execa": "^9.5.2",
    "nuxt": "^3.16.2",
    "typescript": "5.8.3",
    "vitest": "^3.1.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "better-sqlite3",
      "esbuild",
      "vue-demi"
    ]
  }
}
