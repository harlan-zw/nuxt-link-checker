{
  "type": "module",
  "version": "4.3.0",
  "private": true,
  "packageManager": "pnpm@10.11.1",
  "license": "MIT",
  "scripts": {
    "build": "pnpm -r --filter 'packages/**' run build && npm run client:build",
    "dev:prepare": "pnpm -r --filter 'packages/**' run dev:prepare && nuxi prepare client && nuxi prepare playground",
    "client:build": "nuxi generate client",
    "client:dev": "nuxi dev client --port 3030",
    "dev": "npm run play:dev",
    "play:dev": "nuxi dev playground",
    "play:prod": "npm run build && nuxi dev playground",
    "release": "pnpm build && bumpp && pnpm -r publish",
    "lint": "eslint . --fix",
    "test": "vitest run",
    "test:types": "npx nuxi typecheck",
    "test:watch": "vitest watch"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.2",
    "@nuxt/content": "^3.5.1",
    "@nuxt/devtools": "^2.4.1",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/test-utils": "^3.19.1",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/sitemap": "^7.3.1",
    "@types/cheerio": "^1.0.0",
    "bumpp": "^10.1.1",
    "cheerio": "^1.0.0",
    "eslint": "^9.28.0",
    "execa": "^9.6.0",
    "happy-dom": "^17.6.1",
    "nuxt": "^3.17.4",
    "typescript": "^5.8.3",
    "vitest": "^3.2.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@tailwindcss/oxide",
      "better-sqlite3",
      "esbuild",
      "unrs-resolver",
      "vue-demi"
    ]
  }
}
