{
  "name": "nuxt-analyze-eslint-worker",
  "type": "module",
  "version": "0.1.0",
  "description": "ESLint worker for Nuxt Analyze",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harlan-zw/nuxt-analyze.git",
    "directory": "packages/eslint-worker"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./controller": {
      "types": "./dist/controller.d.mts",
      "default": "./dist/controller.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "prepack": "pnpm run build"
  },
  "peerDependencies": {
    "eslint": "^9.0.0"
  },
  "dependencies": {
    "@typescript-eslint/types": "^8.29.0",
    "@typescript-eslint/utils": "^8.29.0",
    "picocolors": "^1.1.1"
  }
}
