{
  "name": "nuxt-link-checker",
  "type": "module",
  "version": "4.3.0",
  "description": "Find and magically fix links that may be negatively effecting your Nuxt sites SEO.",
  "author": {
    "name": "Harlan Wilton",
    "email": "harlan@harlanzw.com",
    "url": "https://harlanzw.com/"
  },
  "license": "MIT",
  "funding": "https://github.com/sponsors/harlan-zw",
  "homepage": "https://github.com/harlan-zw/nuxt-link-checker#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harlan-zw/nuxt-link-checker.git"
  },
  "bugs": {
    "url": "https://github.com/harlan-zw/nuxt-link-checker/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "bin": "./bin/nuxt-analyze.mjs",
  "build": {
    "externals": [
      "unstorage/drivers/fs"
    ]
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt-module-build build",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare",
    "db:migrate:audit": "drizzle-kit generate --schema src/db/audit/schema.ts --out src/db/audit/migrations --dialect sqlite",
    "db:migrate:root": "drizzle-kit generate --schema src/db/root/schema.ts --out src/db/root/migrations --dialect sqlite",
    "db:delete": "rm -rf ./src/db/audit/migrations/* && rm -rf ./src/db/root/migrations/*",
    "db:sync": "pnpm db:delete && pnpm db:migrate:audit && pnpm db:migrate:root",
    "_crawl": "STORAGE_DIR=./.data npx tsx ./src/cli.ts _crawl --url https://harlanzw.com",
    "_analyze": "STORAGE_DIR=./.data/analyze/2 npx tsx ./src/cli.ts _analyze"
  },
  "dependencies": {
    "@nuxt/devtools-kit": "^2.4.1",
    "@nuxt/eslint": "^1.4.1",
    "@nuxt/kit": "^3.17.4",
    "@vueuse/core": "^13.3.0",
    "cac": "^6.7.14",
    "consola": "^3.4.2",
    "crawlee": "^3.13.5",
    "db0": "^0.3.2",
    "destr": "^2.0.5",
    "devalue": "^5.1.1",
    "diff": "^8.0.2",
    "drizzle-kit": "^0.31.1",
    "drizzle-orm": "^0.44.1",
    "eslint-worker-pool": "^0.1.4",
    "fuse.js": "^7.1.0",
    "magic-string": "^0.30.17",
    "mdream": "^0.2.4",
    "nuxt-site-config": "^3.2.0",
    "ofetch": "^1.4.1",
    "ollama": "^0.5.16",
    "pathe": "^2.0.3",
    "pkg-types": "^2.1.0",
    "radix3": "^1.1.2",
    "sirv": "^3.0.1",
    "std-env": "^3.9.0",
    "ufo": "^1.6.1",
    "ultrahtml": "^1.6.0",
    "unstorage": "^1.16.0"
  },
  "publishConfig": {
    "scripts": {},
    "devDependencies": {}
  }
}
